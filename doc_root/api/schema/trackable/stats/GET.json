{
	"type": "object",
	"properties": {
		"startDate": {
			"type": "string",
			"pattern": "^(\\d{4})-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]) ([01]\\d|2[0-3]):([0-5][0-9]):([0-5][0-9])$"
	    	},
		"endDate": {
			"type": "string",
			"pattern": "^(\\d{4})-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]) ([01]\\d|2[0-3]):([0-5][0-9]):([0-5][0-9])$"
		},
		"students": {
			"type": "array",
			"items": {
				"type": "string",
				"pattern": "^[\\d]+$"
			},
			"uniqueItems": true
		},
		"classId": {
			"type": "string",
			"pattern": "^[\\d]+$"
		}
	},
	"oneOf": [
		{
			"required": ["students"]
		},
		{
			"required": ["classId"]
		}
	],
	"additionalProperties": false
}
