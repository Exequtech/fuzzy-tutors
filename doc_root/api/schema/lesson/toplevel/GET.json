{
    "type": "object",
    "properties": {
        "id": {
            "type": "string",
            "pattern": "^[\\d]$"
        },
        "tutorId": {
            "type": "string",
            "pattern": "^[\\d]$"
        },
        "subjectId": {
            "type": "string",
            "oneOf": [
                {
                    "pattern": "^[\\d]$"
                },
                {
                    "enum": ["null"]
                }
            ]
        },
        "locationId": {
            "type": "string",
            "oneOf": [
                {
                    "pattern": "^[\\d]$"
                },
                {
                    "enum": ["null"]
                }
            ]
        },
        "before": {
            "type": "string",
            "pattern": "^(\\d{4})-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]) ([01]\\d|2[0-3]):([0-5][0-9]):([0-5][0-9])$",
            "format": "date-time"
        },
        "after": {
            "type": "string",
            "pattern": "^(\\d{4})-(0[1-9]|1[0-2])-(0[1-9]|[12]\\d|3[01]) ([01]\\d|2[0-3]):([0-5][0-9]):([0-5][0-9])$",
            "format": "date-time"
        },
        "notes": {
            "type": "string",
            "maxLength": 1024
        },
        "orderBy": {
            "type": "string",
            "enum": ["id", "tutorId", "subjectId", "locationId", "startDate", "endDate", "notes"]
        },
        "order": {
            "type": "string",
            "enum": ["asc", "desc"]
        },
        "page": {
            "type": "string",
            "pattern": "^[\\d]$"
        },
        "pageSize": {
            "type": "string",
            "pattern": "^[\\d]$"
        }
    },
    "additionalPropeties": false
}